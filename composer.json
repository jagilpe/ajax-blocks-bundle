{
    "name": "jagilpe/ajax-blocks-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle for rendering blocks refreshable with ajax requests",
    "keywords": [ "Ajax", "Block", "Symfony" ],
    "homepage": "https://github.com/jagilpe/ajax-blocks-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Javier Gil",
            "email": "javier@gilpereda.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/http-kernel": "~3.2",
        "twig/twig": "~1.28|~2.0",
        "symfony/templating": "~2.8|~3.0",
        "symfony/twig-bundle": "~2.8|~3.0",
        "symfony/twig-bridge": "~2.8|~3.0"
    },
    "require-dev": {
        "symfony/asset": "~2.8|~3.0",
        "symfony/browser-kit": "~2.8|~3.0",
        "symfony/console": "~2.8|~3.0",
        "symfony/css-selector": "~2.8|~3.0",
        "symfony/dom-crawler": "~2.8|~3.0",
        "symfony/form": "~2.8|~3.0",
        "symfony/framework-bundle": "~3.1",
        "symfony/http-foundation": "~2.8|~3.0",
        "symfony/security-acl": "~2.8|~3.0",
        "symfony/process": "~2.8|~3.0",
        "symfony/validator": "~2.8|~3.0",
        "symfony/var-dumper": "~3.2",
        "symfony/yaml": "~2.8|~3.0",
        "symfony/expression-language": "~2.8|~3.0",
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Jagilpe\\AjaxBlocksBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
